// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String
}

model Conversation {
  id Int @id @default(autoincrement())
  title String
  recording_url String
  transcript Json
  summary String
  themes String[]
}